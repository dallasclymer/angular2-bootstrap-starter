{
  "name": "angular2-bootstrap-starter",
  "version": "0.35.0",
  "description": "Angular2-bootstrap starter repo",
  "main": "index.js",
  "scripts": {
    "deploy": "NODE_ENV=production webpack -p --progress --color --optimize-minimize --optimize-dedupe --optimize-occurence-order",
    "postinstall": "tsd reinstall --overwrite",
    "prestart": "npm install",
    "server": "webpack-dev-server --hot --inline --colors --display-error-details --display-cached --port 3000",
    "start": "npm run server",
    "test": "gulp lint"
  },
  "keywords": [
    "angular2",
    "n2-bootstrap",
    "angular2-bootstrap",
    "starter",
    "angular2-bootstrap-started"
  ],
  "author": "Dmitriy Shekhovtsov <valorkin@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/valor-software/angular2-bootstrap-starter.git"
  },
  "bugs": {
    "url": "https://github.com/valor-software/angular2-bootstrap-starter/issues"
  },
  "homepage": "https://github.com/valor-software/angular2-bootstrap-starter#readme",
  "dependencies": {
    "angular2": "2.0.0-alpha.37",
    "es6-promise": "^3.0.2",
    "moment": "^2.10.6",
    "ng2-bootstrap": "^0.37.0",
    "reflect-metadata": "^0.1.0",
    "rtts_assert": "2.0.0-alpha.37",
    "traceur-runtime": "0.0.59",
    "zone.js": "^0.5.2"
  },
  "devDependencies": {
    "babel-eslint": "^4.0.10",
    "compression-webpack-plugin": "^0.2.0",
    "eslint": "^1.2.1",
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.0.0",
    "gulp-size": "^2.0.0",
    "gulp-tslint": "^3.2.0",
    "pre-commit": "^1.1.1",
    "require-dir": "^0.3.0",
    "typescript": "^1.5.3",
    "typescript-simple-loader": "^0.3.4",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1"
  }
}
